#include <linux/module.h>
#include <init.h>



static int hello_data __initdata  =1; //只在初始化时候使用，在初始化之后内核释放该空间







static int __init hello_init (void)
{
	printk();

	return 0;
}


static int __init hello_exit(void)
{

	return 0;
}


module_init(hello_init);
module_exit(hello_exit);





